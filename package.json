{
  "name": "nuxt-mitter",
  "version": "1.1.0",
  "description": "Nuxt-Mitter - Nuxt module for mitt library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gianlucazicca/nuxt-mitter.git"
  },
  "keywords": [
    "nuxt-mitter",
    "nuxt3",
    "nuxt-module",
    "mitter",
    "event",
    "event-listener",
    "event-emitter",
    "emitter",
    "event-bus",
    "event-handler",
    "nuxt"
  ],
  "homepage": "https://github.com/gianlucazicca",
  "authors": [
    "Gianluca Zicca <gianluca.dev@proton.me>"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "nuxt-module-build prepare",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "changelogen --release && npm publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.2",
    "mitt": "^3.0.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.5.0",
    "@nuxt/eslint-config": "^0.5.7",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "@nuxt/test-utils": "^3.14.2",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.11.0",
    "nuxt": "^3.13.0",
    "typescript": "latest",
    "vitest": "^2.1.1",
    "vue-tsc": "^2.1.6"
  },
  "packageManager": "pnpm@9.12.0+sha512.4abf725084d7bcbafbd728bfc7bee61f2f791f977fd87542b3579dcb23504d170d46337945e4c66485cd12d588a0c0e570ed9c477e7ccdd8507cf05f3f92eaca"
}
